<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>builds the marvin testing framework for cloudstack. Marvin is written in python and leverages python&apos;s unittest2 for writing tests. &#xd;
This job provides a packaged source tarball for any of the cloudstack branches</description>
  <logRotator>
    <daysToKeep>7</daysToKeep>
    <numToKeep>10</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>BRANCH_NAME</name>
          <description>provide the CloudStack branch for which you need Marvin eg:  2.2.y, 3.0.x</description>
          <defaultValue>master</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd /home/prasanna/cloudstack-oss
git checkout $BRANCH_NAME
ant package-marvin</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>/home/prasanna/cloudstack-oss/tools/marvin/dist/Marvin*.tar.gz</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <org.jenkinsci.plugins.emotional__jenkins.EmotionalJenkinsPublisher/>
  </publishers>
  <buildWrappers/>
</project>